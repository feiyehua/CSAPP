Npos9:
	iaddq $72, %rsi
	andq %r10, %r10		# val <= 0?
	jle Npos10
	iaddq $1, %rax		# count++
Npos10:
	iaddq $-9, %rdx

	jge Unrolled_loop	# if len > 0, goto Unrolled_loop:



Two_check:
	iaddq $8, %rdx
	jg Unrolled_loop_2
	je Loop		# if so, goto Loop Once:
	ret

Unrolled_loop_2_with_offset_mod:
	iaddq $16, %rsi		# dst++
	iaddq $16, %rdi		# src++
