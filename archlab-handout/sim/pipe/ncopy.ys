	jle Npos4_2		# if so, goto Npos2_2:
	iaddq $1, %rax		# count++
Npos4_2:

	iaddq $-2, %rdx
	jg Unrolled_loop_2_with_offset_mod	# if len > 0, goto Unrolled_loop_2:



Zero_check:
	andq %rdx, %rdx
	# Perform the rest iterations
	je Loop_2:		# if so, goto Done:
	ret

Loop:
	mrmovq (%rdi), %r10	# read val from src...
	rmmovq %r10, (%rsi)	# ...and store it to dst
	andq %r10, %r10		# val <= 0?
