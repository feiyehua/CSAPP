	jle Npos8		# if so, goto Npos8:
	iaddq $1, %rax		# count++
Npos8:
	iaddq $8, %rsi		# dst++

	iaddq $-8, %rdx
	jge Unrolled_loop	# if len > 0, goto Unrolled_loop:

Two_check:
	iaddq $7, %rdx
	jg Unrolled_loop_2
	je Loop
	ret


Unrolled_loop_2:
	mrmovq (%rdi), %r10	# read val from src...
